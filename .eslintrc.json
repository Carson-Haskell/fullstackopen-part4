{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true,
    "jest": true
  },
  "extends": "airbnb",
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "no-console": 0,
    "operator-linebreak": 0,
    "no-param-reassign": 0,
    "no-underscore-dangle": 0,
    "object-curly-newline": 0
  }
}
